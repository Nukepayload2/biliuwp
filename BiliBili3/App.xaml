                                <Button x:Name="NextButtonVertical" Visibility="Collapsed"  Opacity="0" HorizontalAlignment="Center" Height="20" IsTabStop="False" Template="{StaticResource VerticalNextTemplate}" UseSystemFocusVisuals="False" VerticalAlignment="Bottom" Width="36"/>
